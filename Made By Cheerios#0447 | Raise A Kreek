-- just putted this on a loadstring, so you guys wont kill yourself reading the code

local args = {
    [1] = game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.FrameShop.UpgradesShop.SilverCoin.Purchase.Cost,
    [2] = game:GetService("Players").LocalPlayer.leaderstats.Cash
}

game:GetService("ReplicatedStorage").BuySilverCoin:FireServer(unpack(args))
wait(2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(428, 3, 778)
wait(1)
local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").ChangeLand:FireServer(unpack(args))
print("waiting for map to load..")
game.StarterGui:SetCore("SendNotification", {Title = "About your mum"; Text = "waiting for map to load.."; Icon = "rbxassetid://57254792"; Duration = 5;})
wait(5)

for i,v in pairs(game:GetService("Workspace").CustomIslands.Island.MapItems:GetDescendants()) do
	if v:IsA("ClickDetector") then
		fireclickdetector(v)
	end
end
wait(1)
local args = {
    [1] = 9.e+14,
    [2] = game:GetService("Players").LocalPlayer.leaderstats.Cash
}

game:GetService("ReplicatedStorage").SellLadybug:FireServer(unpack(args))

